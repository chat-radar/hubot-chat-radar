{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "curly": false,
    "no-console": [true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]
  }
}
