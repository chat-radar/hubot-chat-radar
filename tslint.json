{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "no-console": [true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "curly": false,
    "no-string-literal": false,
    "ordered-imports": false
  }
}
